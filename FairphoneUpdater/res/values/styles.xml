    </style>

    <style name="TextRegular14GreenDark" parent="@style/TextRegular14">
        <item name="android:textColor">@color/green_dark</item>
    </style>

    <style name="TextRegular16Blue" parent="@style/TextRegular16">
        <item name="android:textColor">@color/blue</item>
    </style>

    <style name="TextRegular16Green" parent="@style/TextRegular16">
        <item name="android:textColor">@color/green</item>
    </style>

    <style name="TextRegular16BlueDark" parent="@style/TextRegular16">
        <item name="android:textColor">@color/blue_dark</item>
    </style>

    <style name="TextRegular16White" parent="@style/TextRegular16">
        <item name="android:textColor">@color/white</item>
    </style>

    <!-- TEXT LIGHT -->
    <style name="TextLight">
        <item name="android:fontFamily">sans-serif-light</item>
    </style>

    <style name="TextLight24" parent="@style/TextLight">
        <item name="android:textSize">24sp</item>
    </style>

    <style name="TextLight24BlueDark" parent="@style/TextLight24">
